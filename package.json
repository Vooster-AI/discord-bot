{
  "name": "discord-bot",
  "version": "1.0.0",
  "description": "Discord Bot Server with TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "pnpm prisma generate",
    "prisma:migrate:create": "pnpm prisma migrate dev --create-only",
    "prisma:migrate:deploy": "pnpm prisma migrate deploy",
    "deploy-commands": "ts-node-dev src/bot/deploy-commands.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "prisma"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "discord.js": "^14.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "happy-dom": "^18.0.1",
    "prisma": "^5.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
